{
	"compilerOptions": {
		"target": "ESNext",
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noEmit": false,
		"outDir": "../dist",
		"declaration": false,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"types": ["node"]
	},
	"include": [
		"./**/*.ts"
	]
}
