{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"jsx": "react",
		"jsxFactory": "h",
		"jsxFragmentFactory": "Fragment",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noEmit": true,
		"declaration": false,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"types": [
			"node",
			"@pounce/core"
		],
		"paths": {
			"@pounce": [
				"./.generated-types/@pounce.d.ts"
			],
			"mutts": [
				"../mutts/dist/index.d.ts"
			],
			"mutts/*": [
				"../mutts/dist/src/*"
			],
			"board": [
				"../pounce/packages/board/src/index.ts"
			],
			"board/server": [
				"../pounce/packages/board/src/server/index.ts"
			],
			"board/*": [
				"../pounce/packages/board/src/*"
			]
		}
	},
	"include": [
		"src/**/*.ts",
		"src/**/*.tsx",
		".generated-types/@pounce.d.ts"
	],
	"exclude": []
}